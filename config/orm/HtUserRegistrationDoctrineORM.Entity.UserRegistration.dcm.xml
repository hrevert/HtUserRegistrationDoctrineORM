<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="HtUserRegistrationDoctrineORM\Entity\UserRegistration">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="token" type="string" unique="true" nullable="false"/>
    <field name="requestedAt" type="datetime" nullable="false"/>
    <field name="hasResponded" type="boolean" nullable="false"/>
  </entity>
</doctrine-mapping>
